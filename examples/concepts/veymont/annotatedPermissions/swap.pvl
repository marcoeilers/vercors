class Role {
  int v, temp;
}

choreography swap(int x, int y) {
  endpoint a = Role();
  endpoint b = Role();

  context Perm(a.v, 1) ** Perm(a.temp, 1);
  context Perm(b.v, 1) ** Perm(b.temp, 1);
  run {
    a.v := x;
    b.v := y;
    communicate a.v -> b.temp;
    communicate b.v -> a.temp;
    a.v := a.temp;
    b.v := b.temp;
  }
}